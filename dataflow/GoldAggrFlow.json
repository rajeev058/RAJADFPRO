{
	"name": "GoldAggrFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "adlsls",
						"type": "LinkedServiceReference"
					},
					"name": "DmAirline"
				},
				{
