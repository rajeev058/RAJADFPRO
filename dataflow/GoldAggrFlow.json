				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "adlsls",
						"type": "LinkedServiceReference"
					},
					"name": "SinkGoldAirline",
					"rejectedDataLinkedService": {
						"referenceName": "adlsls",
